@model Cabang
@using Syncfusion.EJ2

<div>
    <input name="isNew" value="false" hidden="hidden" />
    <input asp-for="Id" value="@Model.Id" hidden="hidden" />
    <div class="form-row">
        <div class="form-group col-md-12">
            <div class="e-float-input e-control-wrapper">
                <input asp-for="Name" value="@Model.Name" />
                <span class="e-float-line"></span>
                <label asp-for="Name" class="e-float-text e-label-top">Nama</label>
            </div>
        </div>
    </div>

    <div class="form-row">
        <div class="form-group col-md-12">
            <div class="e-float-input e-control-wrapper">
                <textarea asp-for="Address" value="@Model.Address"></textarea>
                <span class="e-float-line"></span>
                <label asp-for="Address" class="e-float-text e-label-top">Alamat</label>
            </div>
        </div>
    </div>



</div>


<ejs-scripts></ejs-scripts>

