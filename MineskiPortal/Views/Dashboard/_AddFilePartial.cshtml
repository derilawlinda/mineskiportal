@model FileModel
@using Syncfusion.EJ2

    <div>
        <input name="isNew" value="true" hidden="hidden" />
        <input name="Id" value="@ViewBag.maxId" hidden="hidden" />
        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Section" />
                    <span class="e-float-line"></span>
                    <label asp-for="Section" class="e-float-text e-label-top">Section</label>
                    <div class="error-position" id="sectionError"></div>
                    <label class="e-error" for="Section" id="Section-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Procedure" />
                    <span class="e-float-line"></span>
                    <label asp-for="Procedure" class="e-float-text e-label-top">Procedure</label>
                    <div class="error-position" id="procedureError"></div>
                    <label class="e-error" for="Procedure" id="Procedure-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Form" />
                    <span class="e-float-line"></span>
                    <label asp-for="Form" class="e-float-text e-label-top">Form</label>
                    <div class="error-position" id="formError"></div>
                    <label class="e-error" for="Form" id="Form-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Title" />
                    <span class="e-float-line"></span>
                    <label asp-for="Title" class="e-float-text e-label-top">Title</label>
                    <div class="error-position" id="titleError"></div>
                    <label class="e-error" for="Title" id="Title-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Element" />
                    <span class="e-float-line"></span>
                    <label asp-for="Element" class="e-float-text e-label-top">Element</label>
                    <div class="error-position" id="elementError"></div>
                    <label class="e-error" for="Element" id="Element-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input type="file" asp-for="URL" />
                    <span class="e-float-line"></span>
                    <label asp-for="URL" class="e-float-text e-label-top">File</label>
                    <div class="error-position" id="fileError"></div>
                    <label class="e-error" for="File" id="File-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="EksternalUrl" />
                    <span class="e-float-line"></span>
                    <label asp-for="EksternalUrl" class="e-float-text e-label-top">Eksternal URL</label>
                    <div class="error-position" id="userError"></div>
                    <label class="e-error" for="EksternalUrl" id="EksternalUrl-info" style="display: none;"></label>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <div class="e-float-input e-control-wrapper">
                    <input asp-for="Work" />
                    <span class="e-float-line"></span>
                    <label asp-for="Work" class="e-float-text e-label-top">Work</label>
                    <div class="error-position" id="userError"></div>
                    <label class="e-error" for="Work" id="Work-info" style="display: none;"></label>
                </div>
            </div>
        </div>


    </div>


<ejs-scripts></ejs-scripts>