@using Syncfusion.EJ2;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Alfapilar - Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <meta name="description" content="Essential JS 2 for ASP.NET Core (Preview)" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="author" content="Syncfusion" />
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Raleway:500,600" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/dasboard/styles.css" rel="stylesheet" />
    <link href="~/css/dasboard/index.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/js-signals/1.0.0/js-signals.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/elasticlunr/0.9.6/elasticlunr.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/crossroads/0.12.0/crossroads.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/hasher/1.2.0/hasher.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
    <script src="~/scripts/lightpick.js"></script>
    <script type="text/javascript">
        if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
            document.write('<script src="scripts/bluebird.min.js"><\/script>');
        }
    </script>
    <script src="~/scripts/ej2.min.js"></script>
    <script>
        var dateEvent;
        function onChange() {
            dateEvent = this.getSelectedRange();
        }
    </script>
</head>
<div id="dialog" class="expense-dialog">
</div>
<div id="confirmDialog" style='min-width: 260px;'></div>
<body>
    @RenderBody()
    <script src="~/scripts/index.js"></script>
    <script src="~/scripts/dashboard.js"></script>
    <script src="~/scripts/userEvent.js"></script>

    <ejs-scripts></ejs-scripts>
</body>
</html>